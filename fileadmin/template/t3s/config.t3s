config {
	domain = {$site.baseurl}
	debug = {$site.debug}

	doctype = html5

	// Character sets
	renderCharset = utf-8
	metaCharset = utf-8

	// URL Settings
	tx_realurl_enable = 1
}

# Set baseURL setting for http or https
config.baseURL = http://{$config.domain}/
[globalString = IENV:TYPO3_SITE_URL = https://{$config.domain}/]
config.baseURL = https://{$config.domain}/
[global]